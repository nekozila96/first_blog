[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=root
Group=nginx
WorkingDirectory=/home/kun/projects/first_blog
ExecStart=/home/kun/projects/first_blog/my_env/bin/gunicorn \
	--access-logfile - \
	--workers 3 \
	--bind unix:/home/kun/projects/first_blog/mysite_3.sock \
	mysite_3.wsgi:application

[Install]
WantedBy=multi-user.target
