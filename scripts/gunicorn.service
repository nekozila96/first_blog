[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=jenkins
Group=nginx
WorkingDirectory=/var/lib/jenkins/workspace/Django
ExecStart=/var/lib/jenkins/workspace/Django/my_env/bin/gunicorn \
	--access-logfile - \
	--workers 3 \
	--bind unix:/var/lib/jenkins/workspace/Django/mysite_3.sock \
	mysite_3.wsgi:application

[Install]
WantedBy=multi-user.target
